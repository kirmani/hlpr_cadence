<?xml version="1.0"?>
<launch>

    <!-- Spawns a simple purple cylinder -x 1.0 y -0.2 -z 1.99 -->  
    <node name="spawn_rc" pkg="gazebo_ros" type="spawn_model" args="-urdf -file $(find simulation)/objects/purple_cylinder.urdf -model cylinder   -x 2.2 -y 1.3 -z 0.99 " respawn="false" output="screen" /> 
</launch>


